{
  "name": "sms",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "dataloader": "^1.3.0",
    "graphql": "^0.11.7",
    "graphql-relay": "^0.5.3",
    "lodash": "^4.17.4",
    "qs": "^6.5.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-relay": "^1.4.1",
    "react-scripts": "1.0.14",
    "relay-local-schema": "^0.6.2",
    "relay-runtime": "^1.4.1"
  },
  "scripts": {
    "start": "node ./setup && react-scripts start",
    "build": "node ./setup && react-scripts build",
    "test": "node ./setup && react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "relay": "relay-compiler --src ./src --schema ./src/data/schema/schema.graphql",
    "update-schema": "babel-node ./src/data/schema/updateSchema.js",
    "build-fixtures": "yarn update-schema && yarn relay",
    "lint": "prettier --write '{src,__{tests,mocks}__}/**/*.js'"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-relay": "^1.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.57.3",
    "prettier": "1.7.4",
    "relay-compiler": "^1.4.1"
  }
}
